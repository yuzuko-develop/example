{
  "Logging": {
    "LogLevel": {
      "Default": "None"
    },
    "ZLoggerConsole": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "ZLoggerRollingFile": {
      "LogLevel": {
        "Default": "Information",
